                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <HBox alignment="BOTTOM_CENTER" prefHeight="34.0" prefWidth="410.0" GridPane.rowIndex="1">
                    <children>
                        <Label fx:id="locationLabel" alignment="BOTTOM_CENTER" text="     Location:              " />
                        <JFXComboBox fx:id="locationChoice" prefHeight="27.0" prefWidth="182.0" />
                    </children>
                </HBox>
                <HBox alignment="BOTTOM_CENTER" GridPane.rowIndex="2">
                    <children>
                        <Label fx:id="makeLabel" alignment="BOTTOM_CENTER" contentDisplay="CENTER" text="          Make:              " />
                        <JFXComboBox fx:id="makeChoice" prefHeight="27.0" prefWidth="182.0" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="7">
                    <children>
                  <JFXButton fx:id="backButton" onAction="#backToServiceRequestMain" style="-fx-background-color: #FFD3DF#FFD3DF;" text="Back" textFill="#000000cc">
