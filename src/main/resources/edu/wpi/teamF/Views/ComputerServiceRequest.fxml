                        <JFXButton fx:id="update" onAction="#update" style="-fx-background-color: #FFD3DF#FFD3DF;" text="Update" textFill="#000000cc">
                            <font>
                                <Font name="STFangsong" size="18.0" />
                            </font>
                        </JFXButton>
                    </children>
                </StackPane>
                <JFXTreeTableView fx:id="treeTableComputer" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="99.0" />
                <GridPane fx:id="deletePane" layoutX="217.0" layoutY="70.0" AnchorPane.rightAnchor="10.0">
                    <columnConstraints>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="SOMETIMES" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <JFXTextField fx:id="deleteText" alignment="BOTTOM_LEFT" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM">
                            <GridPane.margin>
                                <Insets />
                            </GridPane.margin>
                        </JFXTextField>
                        <JFXButton fx:id="delete" alignment="CENTER" onAction="#delete" style="-fx-background-color: #FFD3DF#FFD3DF;" text="Delete" textFill="#000000cc">
                            <GridPane.margin>
                                <Insets right="5.0" />
                            </GridPane.margin>
                        </JFXButton>
                    </children>
                </GridPane>
            </children>
