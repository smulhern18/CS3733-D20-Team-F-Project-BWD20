                        <JFXButton fx:id="findpath" onAction="#loadDialogpath" text="How to find a path?">
                           <font>
                              <Font name="Dubai Light" size="24.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="requestservice" onAction="#loadDialogservicerequest" text="How to request a service?">
                           <font>
                              <Font name="Dubai Light" size="24.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="addnodes" onAction="#loadDialogadmin" text="How to add/edit/delete nodes?">
                           <font>
                              <Font name="Dubai Light" size="24.0" />
                           </font>
                        </JFXButton>
                     </children>
                  </VBox>
               </children>
            </StackPane>
         </children>
      </GridPane>
   </children>
